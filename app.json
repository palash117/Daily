{
  "expo": {
    "name": "daily",
    "slug": "daily",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./public/icons/daily_512.png",
    "splash": {
      "image": "./public/daily_1024.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "VIBRATE",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "package": "com.palash117.daily",
      "versionCode": 2
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    },
    "description": ""
  }
}
